}

.nxl-fragment-comments{
  background-color: #fafafa;
  padding-bottom: 70px;
  >.comments{
    border-bottom: #D7DDE4 1px dotted;
    padding-bottom: 40px;
    overflow: hidden;
    >div{
      width: 94.89528795811518%;
      >div{
        &.content{
          width: 91.44827586206897%;
          margin: auto;
          overflow: hidden;
        }
      }
      &.comments-subject{
        background-color: #fff;
        margin: 17px auto 0 auto;
        padding-bottom: 20px;
        >.content{
          >.title{
            font-size: 1.125em;
            font-weight: bold;
            margin-top: 24px;
          }
          >.author{
            margin-top: 12px;
            height: 40px;
            padding-left: 4px;
            >img{
              width: 40px;
              height: 40px;
              float: left;
            }
            >.text{
              float: left;
              margin-left: 13px;
              >p{
                text-align: left;
                font-size: 0.875em;
                &.name{
                  color: #ff9185;
                }
                &.time{
                  color: #656d78;
                  margin-top: 6px;
                }
              }
            }
          }
          >.des{
            font-size: 0.875em;
            color: #aab2bd;
            text-align: left;
            margin-top: 17px;
            line-height: 1.8;
          }
          >.info{
            display: inline-block;
            font-size: 0.875em;
            float: left;
            color: #656d78;
            margin-top: 23px;
          }
          >.praise{
            float: right;
            width: 9.93103448275862%;
            padding: 1.10344827586207% 0;
            font-size: 0.875em;
            font-family: inherit;
            color: #aab2bd;
            margin-top: 3px;
            border: 1px solid #cdd0d9;
            background-color: transparent;
            border-radius: 3px;
            cursor: pointer;
            &:hover{
              color: #fff;
              background-color: #ff9180;
              border-color: #ff9180;
            }
          }
        }
      }
      &.comments-item{
        border-top: 1px solid #f4f4f4;
        overflow: hidden;
        margin: auto;
        padding-bottom: 20px;
        >.content{
          >.author{
            margin-top: 25px;
            height: 40px;
            padding-left: 4px;
            >.portrait{
              float: left;
              >img{
                width: 40px;
                height: 40px;
              }
              >p{
                margin-top: 10px;
                color: #656d78;
                font-size: 0.875em;
                text-align: center;
              }
            }
            >.text{
              float: left;
              margin-left: 13px;
              >p{
                text-align: left;
                font-size: 0.875em;
                &.name{
                  color: #ff9185;
                }
                &.time{
                  color: #656d78;
                  margin-top: 6px;
                }
              }
            }
          }
          >.des{
            margin-top:15px;
            color: #aab2bd;
            text-align: left;
            font-size: 0.875em;
            word-wrap: break-word;
            padding-left: 57px;
          }
          >.reply{
            float: right;
            width: 9.93103448275862%;
            padding: 1.10344827586207% 0;
            font-size: 0.875em;
            font-family: inherit;
            color: #aab2bd;
            margin: 20px 17px 0 0;
            border: 1px solid #cdd0d9;
            background-color: transparent;
            border-radius: 3px;
            cursor: pointer;
            &:hover{
              color: #fff;
              background-color: #ff9180;
              border-color: #ff9180;
            }
          }
          >.thank{
            float: right;
            width: 9.93103448275862%;
            padding: 1.10344827586207% 0;
            font-size: 0.875em;
            font-family: inherit;
            color: #aab2bd;
            margin-top: 20px;
            border: 1px solid #cdd0d9;
            background-color: transparent;
            border-radius: 3px;
            cursor: pointer;
            &:hover{
              color: #fff;
              background-color: #ff9180;
              border-color: #ff9180;
            }
          }
        }
        &:nth-child(2){
          border: none;
        }
      }
      &.comments-more{
        text-align: center;
        >a{
          color: #aab2bd;
          font-size: 1.125em;
          text-decoration: none;
          &:hover,&:visited,&:focus,&:active{
            text-decoration: none;
          }
        }
      }
      &.comments-submit{
        margin: 26px auto 0;
        >input{
          width: 80.11049723756906%;
          width: @inputwidthhack;
          border: 1px solid #e6e9ee;
          border-radius: 2px;
          color: #aab2bd;
          font-size: 1.125em;
          font-family: inherit;
          padding: 20px 27px;
          outline: none;
//          line-height: 1;
        }
        >button{
          float: right;
          width: 18.92265193370166%;
          background-color: #ffbaae;
          border-radius: 2px;
          padding: 21px 0;
          color: #fff;
          font-size: 1.5em;
          font-family: inherit;
          border: none;
          margin-left: 0.69060773480663%;
          outline: none;
          cursor: pointer;
          text-align: center;
          white-space: nowrap;
          vertical-align: middle;
          box-sizing: border-box;
          line-height: 1;
        }
      }
    }
    &:nth-last-child(1){
      border: none;
    }
  }
}

