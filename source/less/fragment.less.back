.nxl-fragment-quanquan{
  padding: 0;
  letter-spacing: -0.31em;
  .articles{
    width: 49.08376963350785%;
    overflow: hidden;
    background-color: #fafafa;
    display: inline-block;
    letter-spacing: normal;
    vertical-align: top;
    background-position: 8px 20px;
    background-repeat: no-repeat;
    text-align: right;
    cursor: pointer;
    &:nth-child(2n+1){
      margin: 13px 0.81623036649215% 0 0;
    }
    &:nth-child(2n){
      margin: 13px 0 0 0.81623036649215%;
    }
    &.qq-1{
      background-image: url(../img/qq_1.png);
    }
    &.qq-2{
      background-image: url(../img/qq_2.png);
    }
    &.qq-3{
      background-image: url(../img/qq_3.png);
    }
    &.qq-4{
      background-image: url(../img/qq_4.png);
    }
    >.wrap{
      width: 93.6%;
      margin: 34px auto 24px auto;
      >p{
        &.title{
          text-align: center;
          >a{
            font-size: 1.125em;
            font-weight: bold;
            color: #000;
            text-decoration: none;
            &:hover,&:visited,&:focus,&:active{
              text-decoration: none;
              color: #000;
            }
          }
        }
        &.content{
          font-size: 0.75em;
          color: #656d78;
          text-align: left;
          margin: 23px 0 16px 0;
        }
      }
      >span{
        display: inline-block;
        margin-right: 18px;
        padding: 6px 0;
        width: 45px;
        text-align: right;
        font-size: 12px;
        &.pv{
          background: url(../img/icon_qq_pv.png) no-repeat;
          background-position: 0 center;
          color: #ffbdb4;
        }
        &.comments{
          background: url(../img/icon_qq_commnets.png) no-repeat;
          background-position: 0 center;
          color: #a0d468;
        }
      }
    }
  }
}

